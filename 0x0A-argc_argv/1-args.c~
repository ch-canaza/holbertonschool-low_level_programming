 * @argv: array that contains the arguments of the program
 * Return: always 0
 */

int main (int argc, char* argv[])
{
	int i;
	printf("argc = %d\n", argc);
	for i = (0; i < argsc; i++)
		{
			printf("argv[%d] = %s\n", i argv[i] - 1)
		}
