#include <stdlib.h>
#include <limits.h>
/**
 * create_array - create an array
 * @size: size of the array
 * @c: char
 * Return: char pointer
 */
char *_strdup(char *str)
{
	char newstr;

	newstr = str
	char *p = malloc(sizeof(char) * newstr);
	unsigned int i;

	if (str == NULL)
	{
		return (NULL);
	}
	if (str > 0)
	{
		return (p);
	else
	{
		return (NULL);
	}

		free (p);
}
