#include "dog.h"
/**
 * init_dog-structure
 * @d: pointer to dog struct
 * @name: name of the instance
 * @age: age of the instance
 * @owner: owner of the dog
 * Resturn: 0
 */

void init_dog(struct dog *d, char *name, float age, char *owner)
{
	if (d)
	{
		d->name = name;
		d->age = age;
		d->owner = owner;
	}
}
