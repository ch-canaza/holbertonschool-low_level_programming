#include "holberton.h"

/**
 *_strpbrk - finds the first character in the string s1 that match with s2
 * @s: string to be compared
 *@accept: string to match with s
 *
 *
 *
 *
 */

char *_strpbrk(char *s, char *accept)

{
	int i;
	int j;

	for (i = 0; i != '\0'; i++)

	{
		for (j = 0; j != '\0'; j++)

		{
			if (s[i] == accept[j])

				return (s);
		}

			else
			{
				s = '\0';
			}

			return (s);
		}

}
